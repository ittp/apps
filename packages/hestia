#!/bin/env bash
# https://raw.githubusercontent.com/ittp/apps/main/packages/hestia


outfile="/tmp/install.sh"
url="https://raw.githubusercontent.com/hestiacp/hestiacp/release/install/hst-install.sh"
wget $url -O $outfile

# https://raw.githubusercontent.com/hestiacp/hestiacp/release/install/hst-install.sh -O /tmp/install.sh
chmod +X $outfile && bash $outfile
